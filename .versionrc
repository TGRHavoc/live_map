{
    "header": "# Changelog\n",
    "commit-all": true,
    "bumpFiles": [
        {
            "filename": "fxmanifest.lua",
            "updater": ".standard-version/fx-updater.js"
        },
        {
            "filename": "version.json",
            "updater": ".standard-version/json.js"
        },
        {
            "filename": "src/live_map/live_map.csproj",
            "updater": ".standard-version/dotnet-updater.js"
        }
    ],
    "releaseCommitMessageFormat": "chore(release): v{{currentTag}}",
    "infile": "docs/CHANGELOG.md",
    "scripts": {
        "precommit": "git add dist/"
    },
    "types": [
        {
            "type": "feat",
            "section": "Features"
        },
        {
            "type": "fix",
            "section": "Bug Fixes"
        },
        {
            "type": "refactor",
            "section": "Changes"
        },
        {
            "type": "perf",
            "section": "Performance Changes"
        },
        {
            "type": "docs",
            "section": "Documentation"
        },
        {
            "type": "test",
            "section": "Tests",
            "hidden": true
        },
        {
            "type": "build",
            "section": "Build System",
            "hidden": true
        },
        {
            "type": "ci",
            "section": "Continuous Integration",
            "hidden": true
        }
    ]
}